            message.IsBodyHtml = true;
            message.Body = errorDescription;

           // message.Body = errorDescription;

           // message.Body = "Your package Successfully Complete";
            Dts.TaskResult = (int)ScriptResults.Success;

            //message send
            client.Send(message);
		}

